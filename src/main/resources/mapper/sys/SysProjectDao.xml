<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.sys.dao.SysProjectDao">

    <select id="queryProjectByName" resultType="io.renren.modules.sys.entity.SysProjectEntity">
		select * from sys_project where project_name = #{projName}
	</select>

    <select id="queryProjectByOtherId" resultType="io.renren.modules.sys.entity.SysProjectEntity">
		select * from sys_project where other_id = #{otherId}
	</select>

</mapper>